pacman.app/Contents/PkgInfo: 
	@$(CHK_DIR_EXISTS) pacman.app/Contents || $(MKDIR) pacman.app/Contents 
	@$(DEL_FILE) pacman.app/Contents/PkgInfo
	@echo "APPL????" >pacman.app/Contents/PkgInfo
pacman.app/Contents/Resources/empty.lproj: 
	@$(CHK_DIR_EXISTS) pacman.app/Contents/Resources || $(MKDIR) pacman.app/Contents/Resources 
	@touch pacman.app/Contents/Resources/empty.lproj
	
pacman.app/Contents/Info.plist: 
	@$(CHK_DIR_EXISTS) pacman.app/Contents || $(MKDIR) pacman.app/Contents 
	@$(DEL_FILE) pacman.app/Contents/Info.plist
	@sed -e "s,@SHORT_VERSION@,1.0,g" -e "s,@TYPEINFO@,????,g" -e "s,@ICON@,,g" -e "s,@EXECUTABLE@,pacman,g" -e "s,@TYPEINFO@,????,g" /usr/local/Cellar/qt/4.8.6/mkspecs/unsupported/macx-clang-libc++/Info.plist.app >pacman.app/Contents/Info.plist
